        "dIFrontend": "docker build -t cc_frontend -f Dockerfile.frontend .",
        "dIBackend": "docker build -t cc_backend -f Dockerfile.backend .",
        "dCUp": "docker-compose up",
        "dCDown": "docker-compose down",
        "dockerRmAll": "docker rm $(docker ps -aq)"
    },
