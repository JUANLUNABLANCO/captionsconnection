    "scripts": {
        "ng": "ng",
        "ngStart": "ng serve",
        "ngBuild": "ng build --prod",
        "ngProd": "nodemon ./ngProd.js",
        "ngTest": "ng test",
        "ngLint": "ng lint",
        "ngE2e": "ng e2e",
        "nodeDev": "cross-env NODE_ENV=development nodemon ./src/index.js",
